    </inertial>
    <visual>
      <geometry>
        <mesh filename="package://testcube/meshes/cubo.stl"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://testcube/meshes/cubo.stl"/>
      </geomtry>
    </collision>
  </link>

  <link name="mast_link">
    <inertial>
      <mass value="7.213"/>
