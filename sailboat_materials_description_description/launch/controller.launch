<launch>

<rosparam file="$(find sailboat_materials_description_description)/launch/controller.yaml" command="load"/>
<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="sailboat_materials_description" args="base_mast_joint_position_controller base_rudder_joint_position_controller joint_state_controller "/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
  <remap from="/joint_states" to="/sailboat_materials_description/joint_states"/>
</node>

</launch>