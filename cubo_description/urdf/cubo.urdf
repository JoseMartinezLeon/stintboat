<!-- JOINTS -->

  <joint name="revolute1_joint" type="continuous">
    <origin xyz="-0.3 -0.5 0.15" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="axis1_link_1"/>
    <axis xyz="0.0 1.0 0.0"/>
  </joint>

  <joint name="revolute2_joint" type="continuous">
    <origin xyz="0.3 -0.5 0.15" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="axis2_link_1"/>
    <axis xyz="0.0 1.0 0.0"/>
  </joint>

  <joint name="wheel1_joint" type="fixed">
    <origin xyz="0.0 -0.2 0.0" rpy="0 0 0"/>
    <parent link="axis1_link_1"/>
    <child link="wheel1_link_1"/>
  </joint>

  <joint name="wheel2_joint" type="fixed">
    <origin xyz="0.0 -0.2 0.0" rpy="0 0 0"/>
    <parent link="axis2_link_1"/>
    <child link="wheel2_link_1"/>
  </joint>

  <joint name="wheel3_joint" type="fixed">
    <origin xyz="0.0 1.2 0.0" rpy="0 0 0"/>
    <parent link="axis1_link_1"/>
    <child link="wheel3_link_1"/>
  </joint>

  <joint name="wheel4_joint" type="fixed">
    <origin xyz="0.0 1.2 0.0" rpy="0 0 0"/>
    <parent link="axis2_link_1"/>
    <child link="wheel4_link_1"/>
  </joint>

  <joint name="base_mast_joint" type="fixed">
    <origin xyz="0.0 0.0 1.2" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="mast_link_1"/>
  </joint>

  <joint name="mast_sail_joint" type="continuous">
    <origin xyz="0.0 0.0 1.3" rpy="0 0 0"/>
    <parent link="mast_link_1"/>
    <child link="sail_link_1"/>
    <axis xyz="0.0 0.0 1.0"/>
  </joint>

<!-- PLUGINS -->

  <gazebo>
      <plugin name="BuoyancyPlugin" filename="libBuoyancyPlugin.so">
        <fluid_density>1.225</fluid_density>
      </plugin>
  </gazebo>

<!-- Gazebo Lift Drag Plugin -->
 <gazebo>
    <plugin name="LiftDragPlugin" filename="libLiftDragPlugin.so">
      <air_density>1.225</air_density>
      <cla>0.0881818</cla>
      <cla_stall>-0.0800727</cla_stall>
      <cda>0.00239273</cda>
      <cda_stall>0.02712336</cda_stall>
      <alpha_stall>16.5</alpha_stall>
      <a0>0.0</a0>
      <area>0.825</area>
      <upward>0.0 1.0 0.0</upward>
      <forward>1.0 0.0 0.0</forward>
      <link_name>sail_link_1</link_name>
      <cp>-0.20 0.0 -0.50</cp>
    </plugin>
  </gazebo>

<!-- Gazebo Force/Joint sensor -->
<!-- Enable the Joint Feedback -->
  <gazebo reference="mast_sail_joint">
    <provideFeedback>true</provideFeedback>
  </gazebo>
<!-- The ft_sensor plugin -->
  <gazebo>
    <plugin name="ft_sensor" filename="libgazebo_ros_ft_sensor.so">
      <updateRate>10.0</updateRate>
      <topicName>ft_sensor_topic</topicName>
      <jointName>mast_sail_joint</jointName>
    </plugin>
  </gazebo>
</robot>
